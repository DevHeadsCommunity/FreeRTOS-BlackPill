{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "request": "launch",
            "executable": "./build/freertos.elf",
            "servertype": "openocd",
            "device": "STM32F446RE",
            "gdbPath": "/usr/bin/arm-none-eabi-gdb",
            "interface": "swd",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "svdFile": "./debug.svd",
            "runToEntryPoint": "main",
            // "breakAfterReset": true,
            "armToolchainPath": "/usr/bin",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 180000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        }
    ],
    "compounds": []
}